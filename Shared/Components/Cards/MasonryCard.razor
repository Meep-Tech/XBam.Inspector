<div class="masonry-card" style="height:calc(@(Card.GetHeightValue(Height));">
    @ChildContent
</div>

@code {
    [Parameter]
    public RenderFragment ChildContent {
        get;
        set; 
    }

    [Parameter]
    public Card.Heights Height {
        get;
        set; 
    } = Card.Heights.Middle;
}
